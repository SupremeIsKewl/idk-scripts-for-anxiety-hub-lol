					poseJointC1(neck,nC1,CFrame.new(0,0,0),i*speed)
					poseJointC1(rj,rjC1,CFrame.new(0,0.45,0),i*speed)
					swait()
				end
			end
		end
	end
	if doingMove then
		if x=="spar1" then
			if currentAnim==x then return end
			currentAnim=x
			speed=0.75
			print(x)
