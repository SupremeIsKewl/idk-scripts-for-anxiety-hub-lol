					poseJointC0(neck,nC0,CFrame.Angles(math.rad(5),math.rad(12),math.rad(0)),i*speed)
					poseJointC0(rj,rjC0,CFrame.Angles(math.rad(-14),math.rad(-12),math.rad(-10)),i*speed)
					
