					if grabbingMode then
						poseJointC0(rm,rmC0,CFrame.Angles(math.rad(135),math.rad(0),math.rad(25)),i*speed)
						poseJointC0(lm,lmC0,CFrame.Angles(math.rad(-25),math.rad(0),math.rad(-5)),i*speed)
					end
