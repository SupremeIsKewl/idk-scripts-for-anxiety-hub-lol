						poseJointC0(lm,lmC0,CFrame.Angles(math.rad(99),math.rad(0),math.rad(25)),i*speed)
					end
					poseJointC0(rlm,rlmC0,CFrame.Angles(math.rad(-25),math.rad(0),math.rad(0)),i*speed)
